{
  "name": "quartzjs",
  "version": "1.1.0",
  "description": "A very useful library with fancy small functions",
  "main": "lib/index.js",
  "scripts": {
    "minify_is": "node_modules/terser/bin/terser lib/is/index.js -m -o lib/is/index.min.js -c toplevel=true,drop_console=true -d process.env.DEBUG=true",
    "minify_has": "node_modules/terser/bin/terser lib/has/index.js -m -o lib/has/index.min.js -c toplevel=true,drop_console=true -d process.env.DEBUG=true",
    "minify_tools": "node_modules/terser/bin/terser lib/tools/index.js -m -o lib/tools/index.min.js -c toplevel=true,drop_console=true -d process.env.DEBUG=true",
    "minify_root": "node_modules/terser/bin/terser lib/index.js -m -o lib/index.js -c toplevel=true,drop_console=true -d process.env.DEBUG=true",
    "build": "node_modules/.bin/babel src -d lib --no-comments && npm run minify_is && npm run minify_has && npm run minify_tools && npm run minify_root",
    "prepublish": "npm run build",
    "test": "nyc jasmine",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "Diego Alberto Molina Vera <dracotm25@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DracotMolver/quartz-js"
  },
  "homepage": "https://dracotmolver.github.io/quartz-js/",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.4.5",
    "coveralls": "^3.0.11",
    "eslint": "^6.0.1",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^5.0.1",
    "nyc": "^15.0.1",
    "prettier": "2.0.4",
    "terser": "^4.6.11"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "functional programming",
    "functions",
    "has function",
    "is function",
    "tools"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.2"
  }
}
